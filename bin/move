#!/usr/bin/env ruby

require_relative '../lib/move.rb'
puts "Welcome to Tic Tac Toe!"
board = ["X"," "," "," "," "," "," "," "," "]
def update_array_at_with(array, index, value)
  array[index] = value
end

update_array_at_with(board, 0, "1")
puts "Where would you like to go?"
input = gets.strip


